{
  "name": "contributte/datagrid-skeleton",
  "description": "Datagrid component project skeleton based on Nette Framework and Contributte libraries by @f3l1x & @paveljanda.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton",
    "datagrid"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">= 8.0",
    "nette/application": "~3.1.0",
    "nette/bootstrap": "~3.1.0",
    "nette/caching": "~3.1.0",
    "nette/database": "~3.1.0",
    "nette/di": "^3.0",
    "nette/finder": "~2.5.0",
    "nette/forms": "~3.1.0",
    "nette/http": "~3.1.0",
    "nette/mail": "~3.1.0",
    "nette/security": "~3.1.0",
    "nette/utils": "^3.0",
    "latte/latte": "^3.0",
    "tracy/tracy": "^2.7.0",
    "ublaboo/datagrid": "^6.0",
    "dibi/dibi": "^4.0.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "phpstan/phpstan": "^1.0.0",
    "phpstan/phpstan-dibi": "^1.0.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": [
        "app/"
      ]
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
